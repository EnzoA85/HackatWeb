{% extends 'base.html.twig' %}
{% block title %}Hackat'Agence | Profil{% endblock %}
{% block body %}

<h1>Profil de {{user.nom}} {{user.prenom}}</h1>
<div class="img-thumbnail" style="margin-left:500px;margin-right:500px;background-color:#EAEAEA;">
    <table>
    <thead>
        <tr>
            <th colspan="2">Mes Inscriptions</th>
        </tr>
    </thead>
    <tbody>
        <tr style="font-weight:bold">
            <td>Hackatons</td>
            <td>Date de l'inscriptions</td>
        </tr>
    </tbody>
    <tbody>
        <tr>
            <td>Test1</td>
            <td>Test2</td>
        </tr>
    </tbody>
</table>

</div>
<style>
table,
td {
    border: 1px solid #333;
    margin:auto;
}

td {
    padding-left:10px;
    padding-right: 10px;
}

thead,
tfoot {
    background-color: #333;
    color: #fff;
}
</style>
{% endblock %}